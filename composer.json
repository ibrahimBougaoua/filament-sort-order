{
    "name": "ibrahimbougaoua/filament-sort-order",
    "description": "This is my package filament-sort-order",
    "keywords": [
        "ibrahimBougaoua",
        "laravel",
        "filament-sort-order"
    ],
    "homepage": "https://github.com/ibrahimbougaoua/filament-sort-order",
    "license": "MIT",
    "authors": [
        {
            "name": "Ibrahim",
            "email": "ibrahim.bougaoua@etu.usthb.dz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^2.0|^3.0-stable",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "IbrahimBougaoua\\FilamentSortOrder\\": "src/",
            "IbrahimBougaoua\\FilamentSortOrder\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IbrahimBougaoua\\FilamentSortOrder\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IbrahimBougaoua\\FilamentSortOrder\\FilamentSortOrderServiceProvider"
            ],
            "aliases": {
                "FilamentSortOrder": "IbrahimBougaoua\\FilamentSortOrder\\Facades\\FilamentSortOrder"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
